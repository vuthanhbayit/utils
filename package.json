{
  "name": "@vt7/utils",
  "version": "0.0.8",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @vt7",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Vu Thanh Bay <vuthanhbayit@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "bugs": {
    "url": "https://github.com/vuthanhbayit/utils/issues"
  },
  "homepage": "https://github.com/vuthanhbayit/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuthanhbayit/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "dev": "yarn build -- --watch",
    "start": "esno src/index.ts",
    "build": "tsup",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "yarn lint -- --fix",
    "release": "yarn lint && standard-version && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@types/throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.7.0",
    "@antfu/ni": "^0.7.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^16.7.1",
    "bumpp": "^6.0.6",
    "eslint": "^7.32.0",
    "esno": "^0.9.1",
    "git-ensure": "^0.1.0",
    "standard-version": "^9.3.1",
    "throttle-debounce": "^3.0.1",
    "tsup": "^4.14.0",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
